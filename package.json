{
  "name": "@node-http-discovery/light_version",
  "version": "1.0.0",
  "description": "Tools to discover/call external API with OpenApi documentation, used to replace OG classical http handler",
  "main": "dist/cjs/endPoint.js",
  "module": "dist/esm/endPoint.js",
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "rename": "node ./scripts/fix-msj.js",
    "build": "npm run build:cjs && npm run build:esm "
  },
  "author": "betaucourt@gmail.com",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/cjs/endPoint.js",
      "import": "./dist/esm/endPoint.js"
    }
  },
  "devDependencies": {
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diggers-lab/node-http-discovery/tree/light_version.git"
  },
  "files": [
    "dist/esm",
    "dist/cjs",
    "src/**/*.ts"
  ]
}
